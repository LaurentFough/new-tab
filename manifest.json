{
	"icons": { "16": "assets/img/icon.png",
		"48": "assets/img/icon.png",
	   "128": "assets/img/icon.png" 
	},

	"manifest_version": 2,
	"name": "New Tab clean",
	"description": "New tab clean, anime wallpaper.",
	"version": "0.1",

	"chrome_url_overrides": {
		"newtab": "index.html"
	},

	"browser_action": {
		"default_icon": "assets/img/iron-man-icon.png"
	},

	"permissions": [
		"tabs"
	],

	"background": {
		"scripts": ["assets/js/background.js"],
		"persistent": true
	}
}